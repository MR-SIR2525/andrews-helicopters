{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.apache_wind": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            {
              "viento": "q.is_on_ground && q.skin_id == 2"
            }
          ]
        },
        "viento": {
          "transitions": [
            {
              "default": "(1.0)"
            }
          ],
          "on_entry": [
            "/function apache_ground"
          ]
        }
      }
    }
  }
}